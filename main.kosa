# Structs
struct person {
    num age,
    str name,
    void greet self {
        log "Hello! My name is {} and i am {} years old.", self.name, self.age
    }
}

marek = person {"age": 15, "name": "Marek Pokorný"}
marek.greet()
# Hello! My name is Marek Pokorný and i am 15 years old.

# Struct as a module
struct Math {
    num add num a, num b {
        a + b
    }
}
Math::add 1, 2

